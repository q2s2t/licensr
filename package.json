  "bugs": {
    "url": "https://github.com/quentinrossetti/licensr/issues"
  },
  "homepage": "https://github.com/quentinrossetti/licensr",
  "devDependencies": {
    "chai": "^1.9.1",
    "docco": "^0.6.3",
    "istanbul": "^0.3.0",
    "mocha": "^1.21.4"
  },
  "dependencies": {
    "colors": "^0.6.2",
    "inquirer": "^0.6.0",
    "mkdirp": "^0.5.0",
    "q": "^2.0.2",
    "underscore": "^1.6.0"
  }
